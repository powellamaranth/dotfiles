[user]
	name = Daniel Gatis
	email = danielgatis@gmail.com
[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
	excludesFile = ~/.gitignore
	fsmonitor = true
[interactive]
	diffFilter = diff-so-fancy --patch
[alias]
  hide = update-index --assume-unchanged
  unhide = update-index --no-assume-unchanged
